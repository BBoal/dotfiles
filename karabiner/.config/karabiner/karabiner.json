    {
      "complex_modifications": {
        "parameters": {
          "basic.simultaneous_threshold_milliseconds": 50,
          "basic.to_delayed_action_delay_milliseconds": 500,
          "basic.to_if_alone_timeout_milliseconds": 1000,
          "basic.to_if_held_down_threshold_milliseconds": 500,
          "mouse_motion_to_scroll.speed": 100
        },
        "rules": [
          {
            "description": "Change caps_lock to option. (Post esc if pressed alone)",
            "manipulators": [
              {
                "from": {
                  "key_code": "caps_lock",
                  "modifiers": {
                    "optional": ["any"]
                  }
                },
                "to": [
                  {
                    "key_code": "left_option"
                  }
                ],
                "to_if_alone": [
                  {
                    "key_code": "escape"
                  }
                ],
                "type": "basic"
              }
            ]
          },
          {
            "description": "Change quote to ctrl. (Post quote if pressed alone)",
            "manipulators": [
              {
                "from": {
                  "key_code": "quote"
                },
                "to": [
                  {
                    "key_code": "left_control"
                  }
                ],
                "to_if_alone": [
                  {
                    "key_code": "quote"
                  }
                ],
                "type": "basic"
              }
            ]
          },
          {
            "description": "left option to sitft+option. (down_arrow if pressed alone)",
            "manipulators": [
              {
                "from": {
                  "key_code": "left_option",
                  "modifiers": {
                    "optional": ["any"]
                  }
                },
                "to": [
                  {
                    "key_code": "left_option",
                    "modifiers": ["left_shift"]
                  }
                ],
                "to_if_alone": [
                  {
                    "key_code": "down_arrow"
                  }
                ],
                "type": "basic"
              }
            ]
          },
          {
            "description": "left control to up arrow if pressed alone)",
            "manipulators": [
              {
                "from": {
                  "key_code": "left_control",
                  "modifiers": {
                    "optional": ["any"]
                  }
                },
                "to": [
                  {
                    "key_code": "left_control"
                  }
                ],
                "to_if_alone": [
                  {
                    "key_code": "up_arrow"
                  }
                ],
                "type": "basic"
              }
            ]
          },
          {
            "description": "Quit application by holding command-q",
            "manipulators": [
              {
                "from": {
                  "key_code": "q",
                  "modifiers": {
                    "mandatory": ["command"],
                    "optional": ["caps_lock"]
                  }
                },
                "to_if_held_down": [
                  {
                    "key_code": "q",
                    "modifiers": ["left_command"],
                    "repeat": false
                  }
                ],
                "type": "basic"
              }
            ]
          },
          {
            "description": "Two shift to ctrl+cmd+s",
            "manipulators": [
              {
                "from": {
                  "key_code": "right_shift",
                  "modifiers": {
                    "mandatory": ["left_shift"],
                    "optional": ["caps_lock"]
                  }
                },
                "to": [
                  {
                    "key_code": "s",
                    "modifiers": ["left_command", "left_control"],
