#!/bin/zsh

alias ss='ls -lht'
alias ll='ls -Alh'
alias l='ls'
alias la='ls -A'
alias lr='ls -lRht'

alias c='clear && echo "" && pwd && ls'
alias cl='clear && ls'

alias q="exit"
alias :q="exit"

alias f='fg'

alias mkdir='mkdir -p'

alias rm='trash'

alias ffind='find . -name'

alias r='lf'

alias o='open .'
alias p='cd ~/Documents/Projects'
alias pg='cd ~/.go/src/github.com'
alias df='cd ~/.dotfiles'

alias s='cd ~/Documents/Projects/work/saama'

alias cleanup="find . -name '*.DS_Store' -type f -ls -delete"

alias lynx='lynx -lss="~/.lynx.css"'
